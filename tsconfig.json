{
  "compilerOptions": {
    "lib": ["es5", "es6"],
    "baseUrl": "./",
    "rootDir": "./",
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,

    "strictPropertyInitialization": false
  }
}
